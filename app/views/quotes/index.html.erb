<h1>Listing quotes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Company name</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Address</th>
    <th>Phone lines</th>
    <th>Current internet</th>
    <th>Current carrier</th>
    <th>Message</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quotes.each do |quote| %>
  <tr>
    <td><%= quote.name %></td>
    <td><%= quote.company_name %></td>
    <td><%= quote.phone %></td>
    <td><%= quote.email %></td>
    <td><%= quote.address %></td>
    <td><%= quote.phone_lines %></td>
    <td><%= quote.current_internet %></td>
    <td><%= quote.current_carrier %></td>
    <td><%= quote.message %></td>
    <td><%= link_to 'Show', quote %></td>
    <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
    <td><%= link_to 'Destroy', quote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quote', new_quote_path %>
